<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="model.TaskORM">
        <table name="TASKS" schema="dbo" catalog="BDD_TaskMaker"/>
        <attributes>
            <id name="idNum">
                <column name="id_num"/>
            </id>
            <basic name="name">
                <column name="name"/>
            </basic>
            <basic name="description">
                <column name="description"/>
            </basic>
            <basic name="date">
                <column name="date"/>
            </basic>
            <basic name="status">
                <column name="status"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
